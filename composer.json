{
  "name": "marklanselott/pdf-reader-php",
  "description": "PDF reader",
  "type": "library",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "ParserPDF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParserPDF\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyze": "psalm",
    "dump": "composer dump-autoload"
  }
}